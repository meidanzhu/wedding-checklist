<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Wedding Checklist!</title>
  </head>
  <body>

    <h1>All Vendors</h1>

    <%@vendors.each do |vendor|%>
        <p>Category:<%=vendor.category%></p>
        <p>Name:<%=vendor.name%></p>
        <p>Address:<%=vendor.address%></p>
        <p>Telephone:<%=vendor.telephone%></p>
        <p>Email:<%=vendor.email%></p>
        <%end%>
  
  </body>
</html>